{
  "name": "nock-hook",
  "description": "Convinient wrapper around Nock for HTTP testing",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint '*.js' 'test/**/*.js'",
    "precommit": "npm run lint && npm test",
    "test": "npm run test:ava && npm run test:mocha",
    "test:ava": "ava test/*.ava.js",
    "test:mocha": "mocha test/mocha"
  },
  "engines": {
    "node": ">=4.0"
  },
  "keywords": [
    "tests",
    "nock",
    "replay"
  ],
